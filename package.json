{
  "name": "manifestation-vue",
  "description": "Vue component toolkit and Manifesto mixins for IIIF-aware user interfaces.",
  "version": "0.0.1",
  "author": {
    "name": "Shaun Ellis",
    "email": "shaun@sdellis.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/manifestation-vue.common.js",
  "unpkg": "dist/manifestation-vue.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdellis/manifestation-vue.git"
  },
  "bugs": {
    "url": "https://github.com/sdellis/manifestation-vue/issues"
  },
  "homepage": "https://github.com/sdellis/manifestation-vue#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "normalizr": "^3.2.3",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  }
}
